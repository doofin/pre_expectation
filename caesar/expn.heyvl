domain Exp {
    func exprp(x:Realplus,exponent: Realplus): Realplus

    axiom exp_base forall x:Realplus . exprp(x,0) == 1
    axiom exp_step forall x:Realplus,exponent: Uint. exprp(x,exponent + 1) == x * exprp(x,exponent-1)
}


